    "@types/canvas-confetti": "^1.4.2",
    "@types/node": "^17.0.23",
    "@unocss/reset": "^0.30.11",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.12.0",
    "jsdom": "^19.0.0",
    "minimist": "^1.2.6",
    "pnpm": "^6.32.4",
    "typescript": "^4.6.3",
    "unocss": "^0.30.11",
    "unplugin-auto-import": "^0.6.9",
